{
  "id": "cti-footer-modal",
  "name": "CTI Footer Modal",
  "bundle": "http://localhost:4000/cdn/wc/cti-footer-modal.js",
  "version": "1.0.0",
  "description": "Footer modal variant",
  "adminOnly": false,
  "props": [
    {
      "name": "title",
      "type": "string",
      "default": "Contact",
      "description": "Modal title"
    },
    {
      "name": "body",
      "type": "string",
      "default": "",
